<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    tools:context="com.example.jack.tapjam.Synth">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/layout">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight=".125"
            android:id="@+id/L1"
             >
            <ImageButton
                android:layout_width="match_parent"
                android:layout_height="350dp"
                android:layout_gravity="center_vertical"
                android:id="@+id/imageButton1"
                android:duplicateParentState="true"
                android:background="@drawable/b11"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight=".125"
            android:id="@+id/L2"
            >
            <ImageButton
                android:layout_width="match_parent"
                android:layout_height="310dp"
                android:layout_gravity="center_vertical"
                android:id="@+id/imageButton2"

                android:duplicateParentState="true"
                android:background="@drawable/b12"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight=".125"
            android:id="@+id/L3"
            >
            <ImageButton
                android:layout_width="match_parent"
                android:layout_height="270dp"
                android:layout_gravity="center_vertical"
                android:id="@+id/imageButton3"

                android:duplicateParentState="true"
                android:background="@drawable/b13"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight=".125"
            android:id="@+id/L4"
            >
            <ImageButton
                android:layout_width="match_parent"
                android:layout_height="230dp"
                android:layout_gravity="center_vertical"
                android:id="@+id/imageButton4"

                android:duplicateParentState="true"
                android:background="@drawable/b14"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight=".125"
            android:id="@+id/L5"
            >
            <ImageButton
                android:layout_width="match_parent"
                android:layout_height="190dp"
                android:layout_gravity="center_vertical"
                android:id="@+id/imageButton5"

                android:duplicateParentState="true"
                android:background="@drawable/b15"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight=".125"
            android:id="@+id/L6"

            >
            <ImageButton
                android:layout_width="match_parent"
                android:layout_height="150dp"
                android:layout_gravity="center_vertical"
                android:id="@+id/imageButton6"

                android:duplicateParentState="true"
                android:background="@drawable/b16"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight=".125"
            android:id="@+id/L7"
            >
            <ImageButton
                android:layout_width="match_parent"
                android:layout_height="110dp"
                android:layout_gravity="center_vertical"
                android:id="@+id/imageButton7"

                android:duplicateParentState="true"
                android:background="@drawable/b17"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight=".125"
            android:id="@+id/L8"
            >
            <ImageButton
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:layout_gravity="center_vertical"
                android:id="@+id/imageButton8"

                android:duplicateParentState="true"
                android:background="@drawable/b18"/>
        </LinearLayout>

    </LinearLayout>

</FrameLayout>
